<p id="notice"><%= notice %></p>

<h1>Listing Ordinateurs</h1>

<table>
  <thead>
    <tr>
      <th>Numero</th>
      <th>Description</th>
      <th>Entreprise</th>
      <th>Beneficiaire</th>
      <th>Etat ordinateur</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordinateurs.each do |ordinateur| %>
      <tr>
        <td><%= ordinateur.numero %></td>
        <td><%= ordinateur.description %></td>
        <td><%= ordinateur.entreprise_id %></td>
        <td><%= ordinateur.beneficiaire_id %></td>
        <td><%= ordinateur.etat_ordinateur_id %></td>
        <td><%= link_to 'Show', ordinateur %></td>
        <td><%= link_to 'Edit', edit_ordinateur_path(ordinateur) %></td>
        <td><%= link_to 'Destroy', ordinateur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ordinateur', new_ordinateur_path %>
