<h1>Listing Employe Gere Ordinateurs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Etat ordinateur preced</th>
      <th>Etat ordinateur acquis</th>
      <th>Employe</th>
      <th>Ordinateur</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employe_gere_ordinateurs.each do |employe_gere_ordinateur| %>
      <tr>
        <td><%= employe_gere_ordinateur.date %></td>
        <td><%= employe_gere_ordinateur.etat_ordinateur_preced_id %></td>
        <td><%= employe_gere_ordinateur.etat_ordinateur_acquis_id %></td>
        <td><%= employe_gere_ordinateur.employe_id %></td>
        <td><%= employe_gere_ordinateur.ordinateur_id %></td>
        <td><%= link_to 'Show', employe_gere_ordinateur %></td>
        <td><%= link_to 'Edit', edit_employe_gere_ordinateur_path(employe_gere_ordinateur) %></td>
        <td><%= link_to 'Destroy', employe_gere_ordinateur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

